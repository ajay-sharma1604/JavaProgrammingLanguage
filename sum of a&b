public class Basics{
    public static void main(String args[]){
        int a = 10;
        int b = 50;
        int Sum= a + b;
        System.out.println(Sum);

        a= 50;
        b= 30;
        int Difference= a-b;
        System.out.println(Difference);

        a= 45;
        b= 2;
        float Division= (float)a/b;
        System.out.println(Division);

        a=30;
        b=19;
        int Multiplication = a*b;
        System.out.println(Multiplication);
    }
}
