                                             Majority Element
import java.util.*;

public class Basics {
    public static void main(String[] args) {
        int arr[] = { 1, 3, 2, 5, 1, 3, 1, 5, 1 };
        HashMap<Integer, Integer> map = new HashMap<>();

        for (int i = 0; i < arr.length; i++) {
            if (map.containsKey(arr[i])) { // key already exist
                // add same key with increased frequency
                map.put(arr[i], map.get(arr[i]) + 1);
            } else {
                map.put(arr[i], 1);
            }
        }

        Set<Integer> keySet = map.keySet();
        for (Integer key : keySet) {
            if (map.get(key) > arr.length / 3) {
                System.out.println(key);
            }
        }
    }
}
Output:- 1


                              Short cuts

import java.util.*;

public class Basics {
    public static void main(String[] args) {
        int arr[] = { 1, 3, 2, 5, 1, 3, 1, 5, 1 };
        HashMap<Integer, Integer> map = new HashMap<>();

        for (int i = 0; i < arr.length; i++) {
            // key exist- normal value: doesnt exist - default value
            map.put(arr[i], map.getOrDefault(arr[i], 0) + 1);
        }

        //no need to store key set alag se, we can directly use map.keySet() in the loop
        for (Integer key : map.keySet()) {
            if (map.get(key) > arr.length / 3) {
                System.out.println(key);
            }
        }
    }
}

Output:- 1
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
                                                 Valid Anagram


import java.util.*;

public class Basics {

    public static boolean isAnagram(String s, String t) {

        if(s.length() != t.length()){
            return false;
        }
        HashMap<Character, Integer> map = new HashMap<>();

        // loop on s

        for (int i = 0; i < s.length(); i++) {
            char ch = s.charAt(i);
            // if ch exist = old freq+1, if ch doesnt exist freq =0+1
            map.put(ch, map.getOrDefault(ch, 0) + 1);
        }

        // loop on t

        for (int i = 0; i < t.length(); i++) {
            char ch = t.charAt(i);
            if (map.get(ch) != null) { // ch exists in map
                if (map.get(ch) == 1) {
                    map.remove(ch);
                } else {
                    map.put(ch, map.get(ch) - 1);
                }
            } else {
                return false;
            }
        }

        // check
        return map.isEmpty();
    }

    public static void main(String[] args) {
        String s = "tulip";
        String t = "lipid";
        System.out.println(isAnagram(s, t));
    }
}
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

                                                    HashSet

import java.util.HashSet;

public class Basics {
    public static void main(String[] args) {
        HashSet<Integer> set = new HashSet<>();

        // add

        set.add(1);
        set.add(2);
        set.add(4);
        set.add(2);
        set.add(1);

        System.out.println(set);

        // contains

        if (set.contains(2)) {
            System.out.println("set contains 2");
        }

        if (set.contains(3)) {
            System.out.println("set contains 3");
        }

        // remove
        set.remove(4);
        System.out.println(set);

        // size
        System.out.println(set.size());

        // clear
        set.clear();
        System.out.println(set.size());

        // isEmpty
        System.out.println(set.isEmpty());

    }
}

