import java.util.*;

public class Basics {
    public static void main(String[] args) {
        LinkedHashSet<String> lhs = new LinkedHashSet<>();
        lhs.add("Delhi");
        lhs.add("Mumbai");
        lhs.add("Noida");
        lhs.add("Bengaluru");

        lhs.remove("Delhi");
        System.out.println(lhs);

        /*
         * Iterator it = lhs.iterator();
         * while (it.hasNext()) {
         * System.out.println(it.next());// next value print and automatically update
         * }
         */
    }
}

